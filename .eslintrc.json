{
    "env": {
        "node": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "no-console": "off",
        "semi": ["error", "always"],
        "no-unused-vars": ["warn", { "vars": "all", "args": "none" }]
    },
    "globals": {
        "vscode": true
    }
} 